/loaded ~/.tf/wiley/misc

/bind qui = /input q
/def reedit = /edit %1 = %-1
/def append = /edit %1 = $%1$%2
/def prepend = /edit %1 = %2$%1
/def gods = /quote shout !"gods"
/def grin = emote beams a wickedly evil grin at %{1-everyone}.
/def box = emote stuffs %{1-someone} into a tiny box and sets it aflame.

/bind .ggc = get all.gold corpse
/bind .gg2 = get all.gold 2.corpse
/bind .gg3 = get all.gold 3.corpse
/bind .gg4 = get all.gold 4.corpse
