/bind .ga6 = get all 6.corpse
/bind .ga7 = get all 7.corpse
/bind .ga8 = get all 8.corpse
/bind .ga9 = get all 9.corpse

/bind .sv = save%;score
/bind .wk = wake%;stand
/bind .dwf = drink water fountain
/bind .egc = get meat corpe%;eat meat

/def kz = cast 'anim' corpse%;order zombie sleep%;kill zombie
/def k = kil %*
/bind .ka = kil acolyte
/bind .kd = kil doppleganger
/bind .kf = kil female
/bind .kg = kil goblin
/bind .kk = kil kobold
/bind .km = kil male
/bind .ko = kil orc
/bind .kp = kil priestess
/bind .ks = kil skeleton
/bind .of = /input order followers
/bind .ok = /input order followers kil
/bind .ow = order followers wake%;order followers stand
